{:paths ["src" "resources"]
 :deps {org.clojure/clojure            {:mvn/version "1.12.0"}

        ;; MongoDB Java Driver (official, supports $vectorSearch)
        org.mongodb/mongodb-driver-sync {:mvn/version "5.3.1"}

        ;; HTTP server
        io.pedestal/pedestal.http-kit   {:mvn/version "0.8.1"}

        ;; Component lifecycle
        com.stuartsierra/component      {:mvn/version "1.1.0"}

        ;; HTTP client (for OpenAI API calls)
        clj-http/clj-http               {:mvn/version "3.13.0"}

        ;; JSON
        cheshire/cheshire               {:mvn/version "5.13.0"}

        ;; Logging
        org.slf4j/slf4j-simple          {:mvn/version "2.0.17"}

        ;; Swagger UI static assets
        metosin/ring-swagger-ui         {:mvn/version "5.31.0"}}

 :aliases
 {:run-m {:main-opts ["-m" "indrabrata.zenius-x-skul-id-rag-poc"]}
  :build {:deps {io.github.clojure/tools.build {:mvn/version "0.10.5"}}
          :ns-default build}
  :test {:extra-paths ["test"]
         :extra-deps {org.clojure/test.check {:mvn/version "1.1.1"}
                      io.github.cognitect-labs/test-runner
                      {:git/tag "v0.5.1" :git/sha "dfb30dd"}}}
  :dev-mode {:jvm-opts ["-Dio.pedestal.dev-mode=true"]}}}
